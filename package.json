{
  "name": "app-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev ./app/server.ts",
    "test:all": "nyc mocha --require ts-node/register ./app/tests/*-tests/*.spec.ts --timeout 10000 -r dotenv/config --exit",
    "test:unit": "mocha --require ts-node/register ./app/tests/unit-tests/*.spec.ts --timeout 10000 -r dotenv/config --exit",
    "test:integration": "mocha --require ts-node/register ./app/tests/integration-tests/*.spec.ts --timeout 10000 -r dotenv/config --exit",
    "start": "node ./dist/server.js",
    "build": "tsc -p .",
    "migrate": "npx knex migrate:latest --knexfile ./dist/knexfile.js --env production",
    "seed": "npx knex seed:run --knexfile ./dist/knexfile.js --env production",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "test:watch": "mocha --watch --require ts-node/register ./app/tests/*.spec.ts",
    "db:start": "npm run migrate && npm run seed && npm start"
  },
