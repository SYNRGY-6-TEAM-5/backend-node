{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/semi": "off",
        "react/react-in-jsx-scope": "off"
    }
}